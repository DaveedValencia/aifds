{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "@id": "https://example.com/#org",
      "name": "Example Brand",
      "url": "https://example.com/",
      "logo": {
        "@type": "ImageObject",
        "@id": "https://example.com/#logo",
        "url": "https://cdn.example.com/logo.png",
        "width": 512,
        "height": 512
      },
      "sameAs": [
        "https://www.linkedin.com/company/example-brand",
        "https://x.com/examplebrand"
      ],
      "contactPoint": [
        {
          "@type": "ContactPoint",
          "contactType": "customer support",
          "email": "support@example.com"
        }
      ]
    },
    {
      "@type": "WebSite",
      "@id": "https://example.com/#website",
      "url": "https://example.com/",
      "name": "Example Brand",
      "inLanguage": "en-US",
      "publisher": {
        "@id": "https://example.com/#org"
      },
      "potentialAction": {
        "@type": "SearchAction",
        "target": "https://example.com/search?q={search_term_string}",
        "query-input": "required name=search_term_string"
      }
    },
    {
      "@type": "WebPage",
      "@id": "https://example.com/#webpage",
      "url": "https://example.com/",
      "name": "Example Brand â€” Ecommerce Homepage",
      "description": "Example Brand helps makers find curated supplies and fast shipping with trustworthy pricing.",
      "inLanguage": "en-US",
      "isPartOf": {
        "@id": "https://example.com/#website"
      },
      "primaryImageOfPage": {
        "@id": "https://example.com/#logo"
      },
      "breadcrumb": {
        "@type": "BreadcrumbList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "item": {
              "@id": "https://example.com/",
              "name": "Home"
            }
          }
        ]
      }
    }
  ]
}
